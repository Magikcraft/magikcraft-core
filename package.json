			"prettier --write",
			"git add"
		],
		"*.js": [
			"git add"
		],
		"*.json": [
			"prettier --write",
			"git add"
		]
	},
	"scripts": {
		"test": "jest"
	},
	"repository": "Magikcraft/core",
	"jest": {
		"testEnvironment": "node"
	},
	"license": "Apache-2.0",
	"dependencies": {
		"@types/jasmine": "^3.3.12"
	}
}
